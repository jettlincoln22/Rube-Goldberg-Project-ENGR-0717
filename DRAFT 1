int dirA=12;
int brakeA=9;
int speedA=3;

int forcePIN = A0;
int threshold = 110;
void setup() {
pinMode(dirA, OUTPUT);
pinMode(brakeA, OUTPUT);
pinMode(speedA, OUTPUT);

digitalWrite(brakeA, LOW);
Serial.begin(9600);


}

void loop() {
digitalWrite(brakeA, LOW);

int forceVal=analogRead(forcePIN);
Serial.println(forceVal);
if (forceVal>threshold){
digitalWrite(dirA, HIGH);
analogWrite(speedA, 255);
delay(3000);
//analogWrite(speedA,0);
//digitalWrite(brakeA, HIGH);
analogWrite(speedA, 0);
digitalWrite(brakeA, HIGH);
threshold=550;

}

}
